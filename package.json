{
  "name": "rommage",
  "version": "3.1.1",
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "lint": "eslint src/ && tsc --noEmit",
    "prettify": "prettier --write .",
    "prepare": "husky install"
  },
  "description": "ROM manipulation for Node.js and the browser",
  "author": {
    "name": "kinetikeith",
    "url": "https://github.com/kinetikeith"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kinetikeith/rommage"
  },
  "keywords": [
    "rom",
    "gameboy",
    "gameboy-advance",
    "nes",
    "snes",
    "ips",
    "ups",
    "bps"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.3",
    "rimraf": "^4.1.2",
    "rollup": "^2.79.1",
    "rollup-plugin-ts": "^3.2.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "buffer": "github:BTOdell/buffer",
    "crc": "^4.3.1",
    "lodash-es": "^4.17.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,cjs,mjs,ts,json,md}": "prettier --write --ignore-unknown"
  }
}
